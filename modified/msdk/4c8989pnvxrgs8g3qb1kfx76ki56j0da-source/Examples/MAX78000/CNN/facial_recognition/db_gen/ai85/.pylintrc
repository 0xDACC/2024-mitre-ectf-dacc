[MESSAGES CONTROL]
disable=invalid-name
[TYPECHECK]
ignored-modules = numpy, matplotlib.cm, torch, distiller
ignored-classes = ModelProto
[FORMAT]
max-line-length = 99
[DESIGN]
max-locals=100
max-statements=300
min-public-methods=1
max-branches=100
max-module-lines=5000
max-args=15
max-returns=10
max-attributes=15
max-nested-blocks=10