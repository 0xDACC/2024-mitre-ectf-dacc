            <description>Low Power Timer1 Reset.</description>
            <bitOffset>1</bitOffset>
            <bitWidth>1</bitWidth>
          </field>
          <field derivedFrom="LPTMR0">
            <name>LPUART0</name>
            <description>Low Power UART0 Reset.</description>
            <bitOffset>2</bitOffset>
            <bitWidth>1</bitWidth>
