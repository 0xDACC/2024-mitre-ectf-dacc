            <bitOffset>2</bitOffset>
            <bitWidth>1</bitWidth>
          </field>
          <field derivedFrom="DMA">
            <name>GPIO1</name>
            <description>GPIO1 Reset. Setting this bit to 1 resets GPIO1 pins to their default states.</description>
            <bitOffset>3</bitOffset>
            <bitWidth>1</bitWidth>
          </field>
          <field derivedFrom="DMA">
